s])\n","\n","  tails = torch.from_numpy(np.array(tails))\n","  tails = torch.stack([entities_embeddings[i] for i in tails])\n","\n","  if relations.size == 1:\n","    relations = tails.size()[0]*[relations]\n","\n","  relations = torch.from_numpy(np.array(relations))\n","  relations = torch.stack([relations_embeddings[i] for i in relations])\n","\n","  distances = torch.sqrt(((heads+relations-tails)**2).sum(axis=1))\n","  # Do we need the sqrt in the end?\n","\n","  if sort:\n","    distances = torch.sort(distances)\n","\n","  return distances"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"H4iJgMmAUB51"},"source":["def evaluate(relations_embeddings, entities_embeddings, eval_set, amount = 163000):\n","  with torch.no_grad():\n","    neg_heads, neg_tails, relations = eval_set['head_neg'], eval_set['tail_neg'], eval_set['relation']\n","    heads, tails = eval_set['head'], eval_set['tail']\n","\n","    scores = []\n","\n","\n","    for i, rel in enumerate(relations):\n","      if i>amount:\n","        break\n","        \n","      temporary_scores = d(neg_heads[i], neg_tails[i], rel, entities_embeddings, relations_embeddings, True)[0]\n","\n","      real_score = norm(entities_embeddings[heads[i]] + relations_embeddings[rel] - entities_embeddings[tails[i]])\n","\n","      \n","\n","      rank = 0\n","      for score in temporary_scores:\n","        if score < real_score:\n","          rank += 1\n","        elif real_score > score: \n","          break\n","\n","      scores += [rank+1]\n","\n","      if i%10000 == 0:\n","        print(f\"Evaluating match number {i}, MRR: {np.mean(1/np.array(scores))}\")  \n","    return np.mean(1/np.array(scores))"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"zRs2jOZG_m1D","executionInfo":{"status":"ok","timestamp":1617672370039,"user_tz":-120,"elapsed":790964,"user":{"displayName":"Michael M","photoUrl":"https://lh3.googleuserconte