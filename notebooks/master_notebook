random\n","  else:\n","    # Corrupt tail\n","    tp = random\n","\n","  return embeddings[hp], embeddings[tp]"],"execution_count":null,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"0fjTxjMktq0v"},"source":["# Defining the TransE Algorithm"]},{"cell_type":"code","metadata":{"id":"2vvB1VM5UGTT"},"source":["def learn_trans_e(k, train_set, b, gamma, max_epochs=100, lr=0.001):\n","  \"\"\"TransE algorithm\"\"\"\n","\n","  # Initializing relations embeddings\n","  relations = []\n","  for _ in range(len(set(train_set['relation']))):\n","    rel = initial_vector(k)\n","    rel.data /= norm(rel)\n","    rel.requires_grad = True\n","    relations.append(rel)\n","\n","  # Initializing entities embeddings\n","  entities_set = list(train_set['head'])\n","  entities_set.extend(list(train_set['tail']))\n","  entities_set = set(entities_set)\n","  nr_entities = len(entities_set)\n","\n","  entities = []\n","  for _ in range(nr_entities):\n","    e = initial_vector(k, req_grad=True)\n","    entities.append(e)\n","\n","  # Logging variables\n","  curr_start = 0\n","  curr_batch = 0\n","  curr_epoch = 0\n","  epoch_loss = 0\n","  nr_batches = int(len(train_set) / b)\n","\n","  # Numpy array version of the training set\n","  numpy_train_set = np.array(train_set)\n","\n","  # Main Loop\n","  while curr_epoch < max_epochs:\n","    # Updating entities\n","    for i in range(len(entities)):\n","      entities[i].data /= norm(entities[i])\n","\n","    # Updating epoch and batch infos\n","    if curr_start + b > len(train_set):\n","      curr_start = 0\n","      curr_epoch += 1\n","      curr_batch = 0\n","      epoch_loss = 0\n","\n","    # Getting a training batch\n","    s_batch = get_correct_batch(numpy_train_set, curr_start, b)\n","    curr_start += b\n","    \n","    t_batch = []\n","    for j in range(len(s_batch)):\n","      sample = s_batch[j]\n","      h, l, t = entities[sample[1]],  relations[sample[2]],  entities[sample[4]]\n","      hp, tp = corrupt(numpy_train_set, entities, sample[1], sample[4], nr_entities)\n","      t_batch.append((h, l, t, hp, tp))\n","    curr_batch += 1\n","\n","    # Getting the gradients\n","    loss = torch.tensor(0, dtype=torch.float32)\n","    \n","    for h, l, t, hp, tp in t_batch:\n","      loss_term = gamma + norm(h + l -t) - norm(hp + l - tp)\n","      if loss_term > 0:\n","        loss += loss_term\n","    loss.backward()\n","    epoch_loss += loss.item()\n","\n","    # Updating embeddings w.r.t gradient\n","    for i in range(len(entities)):\n","      if entities[i].grad is not None:\n","        entities[i].data = (entities[i].data - lr * entities[i].grad.data)\n","        entities[i].grad.data.zero_()\n","\n","    for i in range(len(relations)):\n","      if relations[i].grad is not None:\n","        relations[i].data = (relations[i].data - lr * relations[i].grad.data)\n","        relations[i].grad.data.zero_()\n","\n","    # Logging informations\n","    print(\"Epoch: {}/{}\\tBatch: {}/{}\\tEpoch Loss: {:.4f}\\tBatch Loss: {:.4f}\".\n","          format(curr_epoch+1, max_epochs, curr_batch, nr_batches, epoch_loss/(b*curr_batch), loss))\n","\n","  # Returning learned entities and relations\n","  return entities, relations"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"u2nosWF9D49Y","executionInfo":{"status":"ok","timestamp":1617670371157,"user_tz":-120,"elapsed":1696,"user":{"displayName":"Michael M","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GhXS5G7Y8XOsAM2o7_7xf4XI_nDPbq0GP_tO4_J7g=s64","userId":"08878956878756496165"}},"outputId":"9af5802e-338a-4ac6-8e78-bfc965ccb0da"},"source":["len(train_set[\"head\"])"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["4762678"]},"metadata":{"tags":[]},"execution_count":8}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"jdjdR5uV0RS6","executionInfo":{"status":"ok","timestamp":1617671129469