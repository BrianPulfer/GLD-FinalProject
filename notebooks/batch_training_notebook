\n","\n","  return heads, tails\n","\n","def d(heads, relations, tails, entities_embeddings, relations_embeddings, sort = False, from_numpy = False):\n","  '''Distance measured on batch level'''\n","  global device\n","  \n","  heads = torch.tensor(heads, dtype=torch.int32).to(device)\n","  tails = torch.tensor(tails, dtype=torch.int32).to(device)\n","  relations = torch.tensor(relations, dtype=torch.int32).to(device)\n","  \n","  if relations.size() != tails.size():\n","    relations = torch.tensor(tails.size()[0]*[relations]).to(device)\n","\n","  embedded_heads = torch.index_select(entities_embeddings, 0, heads)\n","  embedded_heads.requires_grad = True\n","  embedded_tails = torch.index_select(entities_embeddings, 0, tails)\n","  embedded_tails.requires_grad = True\n","  embedded_relations = torch.index_select(relations_embeddings, 0, relations)\n","  embedded_relations.requires_grad = True\n","\n","  distances = torch.sqrt(((embedded_heads+embedded_relations-embedded_tails)**2).sum(axis=1))\n","  \n","  if sort:\n","    distances = torch.sort(distances)\n","    return distances\n","  \n","  return embedded_heads, embedded_tails, embedded_relations, distances"],"execution_count":6,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"0fjTxjMktq0v"},"source":["# Defining the TransE Algorithm"]},{"cell_type":"code","metadata":{"id":"2vvB1VM5UGTT","executionInfo":{"status":"ok","timestamp":1618568251890,"user_tz":-120,"elapsed":1322,"user":{"displayName":"Michael M","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GhXS5G7Y8XOsAM2o7_7xf4XI_nDPbq0GP_tO4_J7g=s64","userId":"08878956878756496165"}}},"source":["def learn_trans_e(embedding_size, train_set, b, gamma, max_epochs=100, lr=0.001):\n","  \"\"\"TransE algorithm\"\"\"\n","  global device, lengths\n","  # Initializing relations embeddings\n","  \n","  entities = list(train_set['head'])\n","  entities.extend(list(train_set['tail']))\n","  \n","  lengths = {\n","      \"relations\": len(set(train_set['relation'])),\n","      \"entities\": len(set(entities))\n","  }\n","\n","  relations_embeddings = create_embeddings(lengths['relations'], embedding_size).to(device)\n","  relations_embeddings = F.normalize(relations_embeddings, p=2, dim=1)\n","  \n","  entities_embeddings = create_embeddings(lengths['entities'], embedding_size).to(device)\n","  \n","  gamma_vector = torch.tensor(b*[gamma], dtype=torch.float32).to(device)\n","  # Initializing entities embeddings\n","  \n","  # Logging variables\n","  curr_start = 0\n","  curr_batch = 0\n","  curr_epoch = 0\n","  epoch_loss = 0\n","  nr_batches = int(len(train_set) / b)\n","\n","  # Numpy array version of the training set\n","  train_set = np.array(train_set)\n","\n","  # Main Loop\n","  while curr_epoch < max_epochs:\n","    # Normalising entities\n","    entities_embeddings = F.normalize(entities_embeddings, p=2, dim=1)\n","\n","    # Updating epoch and batch infos\n","    if curr_start + b > len(train_set):\n","      curr_start = 0\n","      curr_epoch += 1\n","      curr_batch = 0\n","      epoch_loss = 0\n","\n","    # Getting a training batch batch is sampled batch\n","    batch = get_correct_batch(train_set, curr_start, b)\n","    curr_start += b\n","\n","    heads = batch[:, 1].to(device)\n","    rels = batch[:, 2].to(device)\n","    tails = batch[:, 0].to(device)\n","\n","    heads_corrupt, tails_corrupt = corrupt_vector(heads, tails)\n","\n","    curr_batch += 1\n","    # Getting the gradients\n"," \n","\n","    embedded_heads, embedded_tails, embedded_relations, positive_term = d(heads, rels, tails, entities_embeddings, relations_embeddings)\n","    embedded_heads_corrupt, embedded_tails_corrupt, embedded_relations_corrupt, negative_term = d(heads_corrupt, rels, tails_corrupt, entities_embeddings, relations_embeddings)\n","    loss = gamma_vector + positive_term - negative_term\n","    loss = torch.mean(loss)\n","\n","    #computes dloss/dx for every parameter x which has requires_grad=True.\n","    loss.backward()\n","    epoch_loss += loss.item()\n","\n","    # Updating embeddings w.r.t gradient\n","\n","    #positive term\n","\n","    entities_embeddings = entities_embeddings.index_add_(0, heads, -lr*(embedded_heads.grad))\n","    entities_embeddings = entities_embeddings.index_add_(0, tails, -lr*(embedded_tails.grad))\n","\n","    #negative term\n","    entities_embeddings = entities_embeddings.index_add_(0, heads_corrupt, -lr*(embedded_heads_corrupt.grad))\n","    entities_embeddings = entities_embeddings.index_add_(0, tails_corrupt, -lr*(embedded_tails_corrupt.grad))\n","\n","\n","    #relations\n","    relations_embeddings = relations_embeddings.index_add_(0, rels, -lr*(embedded_relations.grad))\n","    relations_embeddings = relations_embeddings.index_add_(0, rels, -lr*(embedded_relations_corrupt.grad))\n","    if curr_epoch%10==0:\n","      print(curr_epoch, evaluate(relations_embeddings, entities_embeddings, valid_set, 3000).mean())\n","      #print(\"Epoch: {}/{}\\tBatch: {}/{}\\tEpoch Loss: {:.4f}\\tBatch Loss: {:.4f}\".format(curr_epoch+1, max_epochs, curr_batch, nr_batches, epoch_loss, loss))\n","    \n","  # Returning learned entities and relations\n","  return entities_embeddings, relations_embeddings"],"execution_count":13,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"jdjdR5uV0RS6","outputId":"dcffe8d3-84ab-461f-c5ba-4501f21cbcb8"},"source":["entities_embeddings, relations_embeddings = learn_trans_e(300, df, b=100000, gamma=20, max_epochs=100, lr = 0.001)"],"execution_count":null,"outputs":[{"output_type":"stream","text":["/usr/local/lib/python3.7/dist-packages/ipykernel_launcher.py:34: UserWarning: To copy construct from a tensor, it is recommended to use sourceTensor.clone().detach() or sourceTensor.clone().detach().requires_grad_(True), rather than torch.tensor(sourceTensor).\n","/usr/local/lib/python3.7/dist-packages/ipykernel_launcher.py:35: UserWarning: To copy construct from a tensor, it is recomme